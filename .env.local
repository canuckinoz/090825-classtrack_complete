# ClassTrack Client Environment Configuration
# Supabase Configuration - UPDATE WITH YOUR REAL CREDENTIALS
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration
VITE_API_BASE_URL=http://localhost:4000

# Development Authentication Bypass
# Set to 'false' to use normal authentication flow
VITE_DEV_AUTH_BYPASS=false

# Development User Configuration (only used when bypass is enabled)
VITE_DEV_EMAIL=kevin@example.com
VITE_DEV_ROLE=region_admin
VITE_DEV_SCHOOL_ID=demo-school-001

# Application Configuration
VITE_APP_NAME=ClassTrack
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEV_TOOLS=true
